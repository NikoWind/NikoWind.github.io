{
  "name": "hdmp-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && NODE_ENV=production node ./bin/www",
    "watch": "cnpm install && gulp watch",
    "build": "SASS_BINARY_SITE=https://npm.taobao.org/mirrors/node-sass/ cnpm install && NODE_ENV=production gulp build",
    "build-docker": "npm run build && docker build -t 123.56.95.42:5000/hdmp-website  -f Dockerfile .",
    "publish": "npm run build-docker && docker push 123.56.95.42:5000/hdmp-website"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "debug": "~2.6.3",
    "express": "~4.15.2",
