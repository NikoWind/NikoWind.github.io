    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "connect-livereload": "^0.6.0",
    "express": "^4.15.4",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "livereload": "^0.6.2",
    "nodemon": "^1.11.0",
    "nunjucks": "^3.0.1"
  }
